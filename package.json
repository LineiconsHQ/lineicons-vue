{
  "name": "@lineiconshq/vue-lineicons",
  "version": "0.0.3",
  "type": "module",
  "description": "Vue components for LineIcons - Free and Pro icon library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "icons",
    "lineicons",
    "components",
    "free",
    "pro",
    "premium",
    "ui"
  ],
  "author": "LineIcons",
  "license": "MIT",
  "homepage": "https://lineicons.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/LineiconsHQ/lineicons-vue"
  },
  "peerDependencies": {
    "vue": "^3.5.21"
  },
  "dependencies": {
    "@lineiconshq/free-icons": "^0.0.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.5.0",
    "typescript": "^5.3.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
